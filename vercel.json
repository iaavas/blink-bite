{
    "functions": {
      "api": {
        "handler": "server/api/app.ts",
        "rewrites": [{ "source": "/api/(.*)", "destination": "/server/api/app.ts" }]
      }
    },
    "headers": [
      { "source": "/api/(.*)", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
    ]
  }